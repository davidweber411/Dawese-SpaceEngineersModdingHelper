<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.wedasoft.SpaceEngineersModdingHelper.views.UiController">
    <top>
        <MenuBar BorderPane.alignment="CENTER">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#onConfigurationsMenuItemClick" text="Configurations..." />
                  <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" onAction="#onExitMenuItemClick" text="Exit" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </top>
   <left>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <ScrollPane vbarPolicy="ALWAYS">
               <content>
                  <VBox spacing="8.0">
                     <children>
                        <Button fx:id="dashboardButton" layoutX="18.0" layoutY="18.0" mnemonicParsing="false" onAction="#onDashboardButtonClick" prefHeight="60.0" prefWidth="100.0" text="Dashboard (?)" textAlignment="CENTER" wrapText="true" />
                        <Button fx:id="logScannerButton" layoutX="18.0" layoutY="86.0" mnemonicParsing="false" onAction="#onLogScannerButtonClick" prefHeight="60.0" prefWidth="100.0" text="Log scanner (?)" textAlignment="CENTER" wrapText="true" />
                        <Button fx:id="deployModButton" mnemonicParsing="false" onAction="#onDeployModButtonClick" prefHeight="60.0" prefWidth="100.0" text="Deploy mod (?)" textAlignment="CENTER" wrapText="true" />
                        <Button fx:id="createNewCharacterModButton" layoutX="18.0" layoutY="86.0" mnemonicParsing="false" onAction="#onCreateNewCharacterModButtonClick" prefHeight="60.0" prefWidth="100.0" text="Create new character mod (?)" textAlignment="CENTER" wrapText="true" />
                     </children>
                     <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                     </padding>
                  </VBox>
               </content>
            </ScrollPane>
         </children>
      </HBox>
   </left>
   <center>
      <StackPane fx:id="centerStackPane" prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
